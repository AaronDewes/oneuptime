{
    "name": "fyipe/log-php",
    "description": "A fyipe PHP package that send logs from your applications to your fyipe dashboard.",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "HackerBay"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/var-dumper": "^5.1",
        "fzaninotto/faker": "^1.9"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-0": {
            "Fyipe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Fyipe\\Tests": "tests/"
        }
    }
}

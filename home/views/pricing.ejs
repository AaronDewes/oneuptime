  <script src="/js/switch.js"></script>
  <script>
    var el = document.querySelector('.checkbox-switch');

    var annualSelected = false;

    var mySwitch = new Switch(el, {
      onSwitchColor: '#78bf7d',
      offSwitchColor: '#78bf7d',
      onJackColor: '#FFFFFF',
      offJackColor: '#FFFFFF',
      onChange: switchPlanCheckBox
    });

    $(document).ready(function () {
      switchPlanCheckBox();
    })

    function switchPlanCheckBox() {
      annualSelected = !annualSelected;

      if (annualSelected) {
        $("#pro-plus-rate").text("$22");
        $("#pro-plus-description").text("Per user billed anually.");

        $("#annual-text").css("font-weight", 600);
        $("#monthly-text").css("font-weight", 500);
      } else {
        $("#pro-plus-rate").text("$25");

        $("#pro-plus-description").text("Per user billed monthly.");
        $("#monthly-text").css("font-weight", 600);
        $("#annual-text").css("font-weight", 500);
      }

      if (window.location.href.indexOf("localhost") > -1) {
        if (annualSelected) {
          $("#basic-link").attr("onclick", "registerUrl('?plan=plan_CpIYuh9Ui2965K')")
          $("#pro-link").attr("onclick", "registerUrl('?plan=plan_CpIZTQWQiIr6rY')")
          $("#pro-plus-link").attr("onclick", "registerUrl('?plan=plan_CpIbtqozj1UVGs')")
        } else {
          $("#basic-link").attr("onclick", "registerUrl('?plan=plan_CpIUcLDhD1HKKA')")
          $("#pro-link").attr("onclick", "registerUrl('?plan=plan_CpIZEEfT4YFSvF')")
          $("#pro-plus-link").attr("onclick", "registerUrl('?plan=plan_CpIatF9qAmeZLP')")
        }
      } else {
        if (annualSelected) {
          $("#basic-link").attr("onclick", "registerUrl('?plan=plan_CogeLcH7wZEMP1')")
          $("#pro-link").attr("onclick", "registerUrl('?plan=plan_CogfwRVpqoOLO6')")
          $("#pro-plus-link").attr("onclick", "registerUrl('?plan=plan_CoggNmls8dUpDy')")
        } else {
          $("#basic-link").attr("onclick", "registerUrl('?plan=plan_CogdfpBptaPGi9')")
          $("#pro-link").attr("onclick", "registerUrl('?plan=plan_CogeidQkPwkycV')")
          $("#pro-plus-link").attr("onclick", "registerUrl('?plan=prod_Cogffh2xpitVg6')")
        }
      }
    }
  </script>

  <!-- Defer loading of style sheets -->
  <script>
    var loadDeferredStyles = function () {
      var addStylesNode = document.getElementById("deferred-styles");
      var replacement = document.createElement("div");
      replacement = document.createElement("div");
      replacement.innerHTML = addStylesNode.textContent;
      document.body.appendChild(replacement)
      addStylesNode.parentElement.removeChild(addStylesNode);
    };
    var raf = window.requestAnimationFrame || window.mozRequestAnimationFrame ||
      window.webkitRequestAnimationFrame || window.msRequestAnimationFrame;
    if (raf) raf(function () { window.setTimeout(loadDeferredStyles, 0); });
    else window.addEventListener('load', loadDeferredStyles);
  </script>

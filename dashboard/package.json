        "npm-force-resolutions": "0.0.3",
        "redux-mock-store": "^1.5.3",
        "should": "^13.2.3"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/**/*.stories.js",
            "!src/store.js",
            "!src/config.js",
            "!src/routes.js",
            "!src/setupTests.js"
        ],
        "testMatch": [
            "<rootDir>/src/test/puppeteer/**/*.test.js",
            "<rootDir>/src/test/puppeteer/**/*.test.enterprise.js"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "flux": "4.0.0",
        "minimist": "1.2.5",
        "node-fetch": "2.6.1",
        "axios": "0.21.1"
    }

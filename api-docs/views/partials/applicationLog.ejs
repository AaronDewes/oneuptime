  <article id="create-application-log" class="cf-grid-container dev-doc-item-container">
    <section class="col-lg-half col-md-half dev-doc-item">
      <h3>Create Application Log<span class="requestType">POST</span></h3>
      <p>
        <p>Create Application Log by Project ID and Component ID.</p>
      </p>
  
      <div class="attribute-parameter-container">
        <h3>Path</h3>
  
        <ul class="doc-item-detail-container">
          <li>
            <div class="cf-grid-row">
              <div class="col-full">
                <div class="item-title">
                  <span>https://fyipe.com/api/application-log/:projectId/:componentId/create</span>
                </div>
              </div>
            </div>
          </li>
        </ul>
      </div>
  
      <div class="attribute-parameter-container">
        <h3>Path Variables</h3>
  
        <ul class="doc-item-detail-container">
          <li data-location="path">
            <div class="cf-grid-row">
              <div class="col-lg-third-1 pull-left">
                <div class="item-title">
                  <span>projectId</span>
                  <span class="item-type">string</span>
                </div>
              </div>
              <div class="col-lg-third-2 pull-left">
                <div class="item-description">
                  <p>The project id of the project.</p>
                </div>
              </div>
            </div>
          </li>
          <li data-location="path">
            <div class="cf-grid-row">
              <div class="col-lg-third-1 pull-left">
                <div class="item-title">
                  <span>componentId</span>
                  <span class="item-type">string</span>
                </div>
              </div>
              <div class="col-lg-third-2 pull-left">
                <div class="item-description">
                  <p>The component id of the component.</p>
                </div>
              </div>
            </div>
          </li>
        </ul>
      </div>
    </section>
    <section class="col-lg-half col-md-half dev-doc-example">
        <div class="sample-type type-curl">
          <div id="exampleRequest" class="doc-example-item doc-example-request">

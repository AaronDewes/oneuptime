        "chrome-launcher": "^0.14.0",
        "cors": "^2.8.5",
        "custom-env": "^2.0.1",
        "express": "^4.17.1",
        "get-ssl-certificate": "^2.3.3",
        "lighthouse": "^8.0.0",
        "moment": "^2.29.1",
        "node-cron": "^3.0.0",
        "node-fetch": "^2.6.1",
        "ping": "^0.4.1",
        "winston": "^3.3.3",
        "winston-slack-transport": "^2.0.0"
    },
    "devDependencies": {
        "depcheck": "^1.4.2",
        "nodemon": "^2.0.7"
